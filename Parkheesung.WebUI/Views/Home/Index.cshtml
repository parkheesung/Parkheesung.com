@model List<Github>
@{
    Layout = "~/Views/Shared/_SubPage.cshtml";
}
<div class="rows">
    <div class="col-md-12">
        <div class="jumbotron">
            <div id="carousel-example-generic" class="carousel slide" data-ride="carousel">
                <ol class="carousel-indicators">
                    <li data-target="#carousel-example-generic" data-slide-to="0" class="active"></li>
                    <li data-target="#carousel-example-generic" data-slide-to="1" class=""></li>
                    <li data-target="#carousel-example-generic" data-slide-to="2" class=""></li>
                </ol>
                <div class="carousel-inner">
                    <div class="item active"><img src="/Content/img/Main_01.png"></div>
                    <div class="item"><img src="/Content/img/Main4.png"></div>
                    <div class="item"><img src="/Content/img/main_03.png"></div>
                </div>
                <a class="left carousel-control" href="#carousel-example-generic" data-slide="prev">
                    <span class="glyphicon glyphicon-chevron-left"></span>
                </a>
                <a class="right carousel-control" href="#carousel-example-generic" data-slide="next">
                    <span class="glyphicon glyphicon-chevron-right"></span>
                </a>
            </div>
            <div class="jumbotron-contents">
                <h2>Web Programmer</h2>
                <p>I have liked programming from the first time. <br />Now, enjoy the programming in spite of free time.</p>
            </div>
        </div>
    </div>
</div>
<div class="rows">
    <div class="col-md-4">
        <div class="panel panel-default">
            <div class="panel-heading">
                <h3 class="panel-title">Facebook Page</h3>
            </div>
            <div class="panel-body">
                <div class="fb-page" data-href="https://www.facebook.com/parkheesungdev/" data-width="600" data-small-header="false" data-adapt-container-width="true" data-hide-cover="false" data-show-facepile="true"><blockquote cite="https://www.facebook.com/parkheesungdev/" class="fb-xfbml-parse-ignore"><a href="https://www.facebook.com/parkheesungdev/">박희성</a></blockquote></div>
            </div>
        </div>
    </div>
    <div class="col-md-4">
        <div class="alert alert-success alert-dismissable">
            <h4>Contact me!</h4>
            <p>I am using the most SNS Service.</p>
            <p>Contact me if needed.</p>
            <p><a class="btn btn-success" href="/Profile/Link">Show SNS Channel list</a></p>
        </div>
    </div>
    <div class="col-md-4">
        <div class="list-group">
            @if (Model != null && Model.Count > 0)
            {
                foreach (var item in Model)
                {
                    <a href="@(item.AccessURL)" class="list-group-item" target="_blank">
                        <h4 class="list-group-item-heading">@(item.Title)</h4>
                        <p class="list-group-item-text">@(item.Memo)</p>
                    </a>
                }
            }
        </div>
    </div>
</div>
